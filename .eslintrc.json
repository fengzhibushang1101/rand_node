{
    "parserOptions": {
        "ecmaVersion": 2017,
        "ecmaFeatures": {
            "experimentalObjectRestSpread": true
        }
    },
    "rules": {
        "max-len": 0,
        "no-await-in-loop": 0,
        "indent": ["error", 4],
        "space-before-function-paren": ["error", "always"],
        "global-require": 0
    },
    "extends": [
        "airbnb"
    ]
}